{
	"name": "helloworld",
	"displayName": "HelloWorld",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
			"language": "latex",
			"path": "./snippets.json"
			}
		],
		"configuration": {
			"title": "TeXMatrix",
			"properties": {
				"TeXMatrix.max-rows":
				{
					"type": "number",
					"default": 10,
					"description": "the maximum number of rows the generator can handle"
				},
				"TeXMatrix.max-cols":
				{
					"type": "number",
					"default": 10,
					"description": "the maximum number of cols the generator can handle"
				},
				"TeXMatrix.env":
				{
					"type": "string",
					"default": "bmatrix",
					"description": "the latex environment of the generated matrix"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}
